{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/admin",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin-script.js",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin-styles.css",
      "dest": "/api/index.js"
    },
    {
      "src": "/script.js",
      "dest": "/api/index.js"
    },
    {
      "src": "/styles.css",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}